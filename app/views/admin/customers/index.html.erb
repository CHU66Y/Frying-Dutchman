
<center>
<table border =1>

 <tr>
	<th><b>First Name</b></th>
	<th><b>Last Name</b></th>
	<th><b>Address</b></th>
	<th><b>Phone</b></th>
 </tr>

<% @customer.each do |customer| %>

<tr>
<td><%= customer.last_name %></td>
<td><%= customer.first_name %></td>
<td><%= customer.address%></td>
<td><%= customer.phone %></td>
<td><%= link_to 'Edit', edit_admin_customer_path(customer)%></td>
<td><%= link_to 'Destroy',{:action =>"destroy", :controller=> "admin/customers", :id => customer}, confirm: "Are you sure?", method: :delete %></td>
</tr>	

<% end %>

</table>
</center>
